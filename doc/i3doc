sudo bleachbit

// Reset file and folders
sudo find . -type d -print0 | xargs -0 chmod 0755
sudo find . -type f -print0 | xargs -0 chmod 0644

// fix tearing Intel
sudo service gdm3 stop
cd /etc/X11
Xorg -configure
